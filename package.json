{
  "name": "personal-website",
  "description": "Nout Reusken's pesonal website",
  "author": "Nout Reusken",
  "version": "1.0",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/nreusken/PersonalWebsite.git"
  },
  "dependencies": {
    "react": "*",
    "react-dom": "*",
    "react-router-dom": "*",
    "@mui/material": "*", 
    "@emotion/react": "*", 
    "@emotion/styled": "*",
    "@mui/icons-material": "*",
    "prop-types": "*"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "bestzip": "*",
    "eslint-config-google": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "react-scripts": "*"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src --ignore-pattern validity.test.js --ext .js --ext .jsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 13
    },
    "rules": {
      "indent": [
        "warn",
        2
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}